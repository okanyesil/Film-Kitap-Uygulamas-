@model List<Yorum>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <div class="row mt-2">
        <div class="col-md-10">
            @foreach (var item in Model)
            {
                <h5>@item.Yorum1</h5>
            }
        </div>
        <div class="col-md-2">
            @await Component.InvokeAsync("Menu")
        </div>
    </div>
</div>


